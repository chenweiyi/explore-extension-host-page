{
  "name": "explore-extension-host-page",
  "version": "0.0.3",
  "description": "chrome extension for host page",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jonghakseo/chrome-extension-boilerplate-react-vite.git"
  },
  "scripts": {
    "cz": "cz",
    "build": "tsc --noEmit && vite build && pnpm run zip",
    "build:firefox": "tsc --noEmit && cross-env __FIREFOX__=true vite build",
    "build:dev": "cross-env __DEV__=true vite build",
    "build:firefox:watch": "cross-env __DEV__=true __FIREFOX__=true vite build -w",
    "build:hmr": "rollup --config utils/reload/rollup.config.ts",
    "wss": "node utils/reload/initReloadServer.js",
    "unocss": "unocss -w",
    "unocss:once": "unocss",
    "build:once": "vite build",
    "dev": "npm run build:hmr && npm run unocss:once && (run-p wss build:dev unocss)",
    "dev:firefox": "npm run build:hmr && (run-p wss build:firefox:watch)",
    "test": "jest",
    "prepare": "husky install",
    "zip": "node ./scripts/zip.js"
  },
  "type": "module",
  "dependencies": {
    "@ant-design/icons": "^5.4.0",
    "ahooks": "^3.8.0",
    "antd": "^5.19.3",
    "axios": "^1.7.2",
    "clsx": "^2.1.1",
    "construct-style-sheets-polyfill": "^3.1.0",
    "d3": "^7.9.0",
    "dayjs": "^1.11.12",
    "lodash-es": "^4.17.21",
    "octokit": "^3.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "unplugin-icons": "^0.17.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@iconify-json/mdi": "^1.1.67",
    "@rollup/plugin-typescript": "^8.5.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@testing-library/react": "13.4.0",
    "@types/chrome": "0.0.246",
    "@types/d3": "^7.4.3",
    "@types/jest": "^29.5.12",
    "@types/node": "18.15.11",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.2.4",
    "@types/ws": "^8.5.11",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.38.1",
    "@unocss/cli": "0.61.0",
    "@unocss/eslint-config": "0.61.0",
    "@unocss/preset-attributify": "0.61.0",
    "@unocss/transformer-attributify-jsx": "0.61.0",
    "@vitejs/plugin-react": "4.3.1",
    "adm-zip": "^0.5.14",
    "chokidar": "^3.6.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.2.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "fs-extra": "11.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "rollup": "2.79.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "1.62.1",
    "ts-loader": "9.4.2",
    "typescript": "4.9.5",
    "unocss": "0.61.0",
    "unplugin-auto-import": "^0.16.7",
    "vite": "5.3.4",
    "vitest": "^2.0.4",
    "ws": "8.13.0"
  },
  "lint-staged": {
    "*.{js,jsx,cjs,mjs,ts,tsx,cts,mts}": "eslint --cache --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    }
  }
}
